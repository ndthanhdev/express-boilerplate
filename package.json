{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"type": "module",
	"main": "src/main.mjs",
	"repository": "git@github.com:ndthanhdev/express-boilerplate.git",
	"author": "ndthanhdev <ndthanhdev@outlook.com>",
	"license": "MIT",
	"engines": {
		"node": "^18.12.1"
	},
	"lint-staged": {
		"**/*.{js,cjs,mjs}": [
			"eslint --fix"
		],
		"**/*.{json,md,yml}": [
			"prettier --write"
		]
	},
	"scripts": {
		"lint": "eslint --ext .js src",
		"lint:fix": "eslint --ext .js src --fix",
		"test": "jest",
		"dev": "nodemon src/main.js",
		"postinstall": "husky install && yarn db:gen",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable",
		"db:gen": "prisma generate",
		"db:deploy": "prisma migrate deploy",
		"db:reset": "prisma migrate reset"
	},
	"dependencies": {
		"@babel/core": "^7.20.12",
		"@babel/preset-env": "^7.20.2",
		"@babel/register": "^7.18.9",
		"@prisma/client": "4.8.1",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"express-jsdoc-swagger": "^1.8.0",
		"http-status": "^1.6.1",
		"morgan": "^1.10.0",
		"pino-http": "^8.3.1",
		"sequelize": "^6.28.0"
	},
	"devDependencies": {
		"eslint": "^7.32.0 || ^8.2.0",
		"eslint-config-airbnb-base": "latest",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.0",
		"jest": "^29.3.1",
		"lint-staged": "^13.1.0",
		"nodemon": "^2.0.20",
		"pinst": "^3.0.0",
		"prettier": "^2.8.1",
		"prisma": "^4.8.1",
		"supertest": "^6.3.3"
	},
	"packageManager": "yarn@3.3.1"
}
